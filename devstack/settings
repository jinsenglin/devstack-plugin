# Turn on all the template services by default
enable_service mytheme

# Default options
MYTHEME_ICO=${MYTHEME_ICO:-NOTHING}
MYTHEME_LOGO=${MYTHEME_LOGO:-NOTHING}
MYTHEME_LOGO_SPLASH=${MYTHEME_LOGO_SPLASH:-NOTHING}

